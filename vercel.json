{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist",
        "buildCommand": "npm run build",
        "installCommand": "npm install"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/assets/(.*)\\.js",
      "headers": {
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/assets/(.*)\\.mjs",
      "headers": {
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/assets/(.*)\\.css",
      "headers": {
        "content-type": "text/css"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://localhub-backend-so0i.onrender.com/api"
  }
} 